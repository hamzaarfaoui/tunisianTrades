<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <link rel="stylesheet" href="{{asset('template/uikit/uikit.min.css')}}" />
        <title>All TRADE - {% block title %}{% endblock %}</title>
    </head>
    <body>
        

        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="uk-alert-danger" uk-alert>
                        <a class="uk-alert-close" uk-close></a>
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}
        <div class="uk-position-cover uk-overlay uk-overlay-default uk-flex uk-flex-center uk-flex-middle">
                    <div>
                        <div class="uk-card uk-card-default uk-card-large uk-card-body">
                            <fieldset class="uk-fieldset">

                                <legend class="uk-legend"><center>{% block legend %}{% endblock %}</center></legend>
                                {% block fos_user_content %}
                                {% endblock fos_user_content %}
                            </fieldset>
                        </div>
                    </div>
        </div>
        <script src="{{asset('template/uikit/uikit.min.js')}}"></script>
        <script src="{{asset('template/uikit/uikit-icons.min.js')}}"></script>
    </body>
</html>
